{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "types": [
      "astro/env"
    ]
  },
  "include": [
    "src",
    "functions",
    "astro.config.mjs"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}